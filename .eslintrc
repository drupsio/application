{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true,
    "browser": true
  },
  "plugins": ["@typescript-eslint/eslint-plugin", "prettier"],
  "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
